{
    "compilerOptions": {
        "target": "ESNext",
        "module": "commonjs",
        "esModuleInterop": true,
        "types": ["node"],
        "outDir": "dist",
        "moduleResolution": "Node",
        "allowJs": true,
        "noEmit": true,
        "strict": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["./src/index.ts"]
}
